    "@schematics/angular": "11.0.4",
    "strip-json-comments": "3.1.1",
    "typescript": "4.1.2"
  },
  "devDependencies": {
    "@angular/cli": "11.0.4",
    "@types/node": "14.14.12",
    "husky": "4.3.5",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1"
  },
  "lint-staged": {
    "*.{js,json,md,ts}": "prettier --write"
